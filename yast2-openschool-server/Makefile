install-data-local: # $(if $(IS_DEVTOOLS),,install-pot)
	mkdir -p $(DESTDIR)$(docdir)
	@for I in $(extra_COPYRIGHT_files) ;					\
	do									\
	    EX="" ;								\
	    if test -f $(srcdir)/$$I ; then					\
		EX=$(srcdir)/$$I ;						\
	    elif test -f $(LEGALESE_DIR)/$$I ; then				\
		EX=$(LEGALESE_DIR)/$$I ;					\
	    else								\
		echo "Error: No file $$I in $(srcdir)/ or in $(LEGALESE_DIR)/";	\
		exit 1 ;							\
	    fi ;								\
	    echo "$(INSTALL_DATA) $$EX $(DESTDIR)$(docdir)" ;			\
	    $(INSTALL_DATA) $$EX $(DESTDIR)$(docdir) ;				\
	done

dist: package-local
	if [ -d /data1/OSC/home\:openschoolserver/yast2-openschool-server/ ] ; then \
	        cd /data1/OSC/home:openschoolserver/yast2-openschool-server/; \
	        osc up; \
	fi
	if [ -d /data1/OSC/home\:openschoolserver/yast2-openschool-server/ ] ; then \
	        mv package/* /data1/OSC/home:openschoolserver/yast2-openschool-server/ ; \
	        cd /data1/OSC/home:openschoolserver/yast2-openschool-server/ ; \
		osc vc; \
	        osc ci -m "New build version"; \
	fi
	echo $(NRELEASE) > RELEASE

package-local: check-parse $(if $(IS_DEVTOOLS),,$(POT_DST)) $(RPMNAME).tar.bz2
	rm -f package/$(RPMNAME)-*.tar.bz2
	rm -f package/*~
	rm -f package/*.bak
	rm -f package/*.auto
	mv $(RPMNAME).tar.bz2 package/
	if ! test -x $(Y2TOOL); then \
	    echo "$(Y2TOOL): not found."; \
	    echo "You have to install yast2-devtools to making a package"; \
	    false; \
	fi
	cp /usr/share/YaST2/data/devtools/bin/create-spec /tmp/
	cp create-spec /usr/share/YaST2/data/devtools/bin
	here=`pwd`; \
	cd $(srcdir) && for i in $(RPMNAME)*.spec.in; do \
	    newname="`echo "$$i" | sed "s/\.in$$//g"`"; \
	    $(Y2TOOL) create-spec < $$i > $$here/package/$${newname} ; \
	done
	mv /tmp/create-spec /usr/share/YaST2/data/devtools/bin/create-spec

package: package-local
	rm -rf /usr/src/packages/*
	cd /usr/src/packages; mkdir -p BUILDROOT BUILD SOURCES SPECS SRPMS RPMS RPMS/athlon RPMS/amd64 RPMS/geode RPMS/i686 RPMS/pentium4 RPMS/x86_64 RPMS/ia32e RPMS/i586 RPMS/pentium3 RPMS/i386 RPMS/noarch RPMS/i486
	cp package/$(RPMNAME).tar.bz2 /usr/src/packages/SOURCES
	sed -i '/-brp-check-suse/d' package/$(RPMNAME).spec
	rpmbuild -ba package/$(RPMNAME).spec
	for i in `ls /data1/PACKAGES/rpm/noarch/$(RPMNAME)* 2> /dev/null`; do rm $$i; done
	for i in `ls /data1/PACKAGES/src/$(RPMNAME)* 2> /dev/null`; do rm $$i; done
	cp /usr/src/packages/SRPMS/$(RPMNAME)-*.src.rpm /data1/PACKAGES/src/
	cp /usr/src/packages/RPMS/noarch/$(RPMNAME)-*.noarch.rpm /data1/PACKAGES/rpm/noarch/


check-up-to-date check-cvs-up-to-date check-svn-up-to-date:
	if [ -d $(srcdir)/CVS ]; then \
	  cd $(srcdir) && ! cvs -q -n up 2>/dev/null | grep '^[MCAR] ';\
	else \
	  cd $(srcdir) && ! LC_ALL=C svn status --show-updates --quiet | grep -v '^Status against revision' ;\
	fi

check-tagversion:
	@if [ -d $(srcdir)/CVS ]; then \
	  cd $(srcdir) && cvs status -v VERSION | grep -w `$(TAGVERSION) -n` ;\
	else \
	  cd $(srcdir) && svn cat `$(TAGVERSION) --echourl`/VERSION; \
	fi >/dev/null; \
	[ $$? = 0 ] || ( echo "ERROR: Please run 'tagversion' first"; false )

check-parse-old:
	@if [ `find -type f -name "*.ycp" | wc -l` -gt 0 -a "$(RPMNAME)" != "yast2-core" ] ; then \
		echo "Running parseycp..." ; \
		parseycp -q -R ; \
	else \
		echo "Not running parseycp." ; \
	fi

check-parse:
	@echo "Not running parseycp."

check-all-packages:
	@if [ ! -e "$(ALL_PACKAGES)" ]; then				\
		echo ERROR: "Can't find $(ALL_PACKAGES)"; false;	\
	fi
	@if ! grep -q "^$(RPMNAME)[[:space:]]" $(ALL_PACKAGES); then		\
		echo ERROR: "$(RPMNAME) not in $(ALL_PACKAGES)"; false;	\
	fi
	@if grep -q "^$(RPMNAME)[[:space:]]\+.*?" $(ALL_PACKAGES); then	\
		echo ERROR: "$(RPMNAME) info in $(ALL_PACKAGES) contains some unknown (?) fields:";	\
		echo;							\
		grep "^$(RPMNAME)[[:space:]]\+.*?" $(ALL_PACKAGES);	\
		echo;							\
		false;							\
	fi

check-textdomain:
	$(Y2TOOL) check-textdomain $(srcdir)

stable: checkin-stable

checkin-stable: package
	$(Y2TOOL) checkin-stable

# For po/ modules
checkpo:
	test ! -f $(srcdir)/po/Makefile || $(MAKE) -C po checkpo

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
